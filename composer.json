{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.0",
        "php-di/php-di": "^6.0",
        "jenssegers/blade": "^1.2",
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "php-di/slim-bridge": "^3.0",
        "illuminate/support": "^7.0.0",
        "illuminate/database": "^7.4",
        "zeuxisoo/slim-whoops": "^0.7.2",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "files": [
            "./app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app",
            "Boot\\": "bootstrap"
        }
    },
    "scripts": {
        "start": [
            "php -S localhost:8080 router.php"
        ]
    },
    "config": {
        "process-timeout": 0
      }
}
